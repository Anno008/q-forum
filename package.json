    "test:e2e:open": "cross-env npx cypress open --config-file ./cypress/cypress.json",
    "lint": "eslint ./src",
    "lint:types": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
