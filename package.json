{
  "name": "q-forum",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env VITE_APP_TESTS=true vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cross-env VITE_APP_TESTS=true jest --no-cache ./src",
    "test:coverage": "cross-env VITE_APP_TESTS=true jest --no-cache ./src --coverage",
    "test:coverage:open": "opener ./coverage/lcov-report/index.html",
    "test:e2e": "cross-env npx cypress run --config-file ./cypress/cypress.json",
    "test:e2e:open": "cross-env npx cypress open --config-file ./cypress/cypress.json",
    "lint": "eslint ./src",
    "lint:types": "tsc"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-feather": "^2.0.9",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^8.2.0",
    "styled-components": "^5.3.5",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react-dom": "^17.0.14",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "babel-preset-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "flush-promises": "^1.0.2",
    "jest": "^27.5.1",
    "opener": "^1.5.2",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1"
  }
