    "prettier": "^2.6.1",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.6"
